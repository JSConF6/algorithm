{
  "code-runner.executorMap": {
    "cpp": "g++ -std=c++17 -o $fileNameWithoutExt $fullFileName && ./$fileNameWithoutExt"
  },
  "code-runner.runInTerminal": true,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
