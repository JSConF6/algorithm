  "editor.parameterHints.enabled": false,
  "editor.wordBasedSuggestions": "off",
  "editor.inlineSuggest.enabled": false,
  "files.associations": {
    "iostream": "cpp",
    "algorithm": "cpp",
    "ios": "cpp",
    "__bit_reference": "cpp",
    "ostream": "cpp",
    "string": "cpp",
    "list": "cpp",
    "istream": "cpp",
    "stack": "cpp",
    "__split_buffer": "cpp",
    "deque": "cpp",
    "queue": "cpp",
    "regex": "cpp",
    "vector": "cpp",
    "sstream": "cpp"
  }
}
